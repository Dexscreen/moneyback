{
  "cleanUrls": true,
  "rewrites": [
    {
      "source": "/:path*",
      "has": [
        {
          "type": "header",
          "key": "accept",
          "value": "text/html"
        }
      ],
      "missing": [
        {
          "type": "file",
          "path": "/:path*"
        }
      ],
      "destination": "/"
    }
  ]
}
